                                            <form class="form-horizontal">
                                                <fieldset>
                                                    <table id="user" class="table table-bordered table-striped" style="clear: both">
                                                        <tbody>
                                                            <tr>
                                                                <td style="width:35%;">Purchase Price (A)</td>
                                                                <td style="width:65%"><input class="form-control" id="purchasePrice" name="purchasePrice" type="text" placeholder="0.00" onchange="chgPurchasePrice(this.value)" onkeypress="return onlyDotsAndNumbers(this,event);" oncopy="return false" ondrag="return false" ondrop="return false" onpaste="return false"></td>
                                                            </tr>
                                                            <tr>
                                                                <td style="width:35%;">Earnest Deposit (B)</td>
                                                                <td style="width:65%"><input class="form-control" id="earnestDeposit" name="earnestDeposit" type="text" placeholder="0.00" onchange="chgEarnestDeposit(this.value)" onkeypress="return onlyDotsAndNumbers(this,event);" oncopy="return false" ondrag="return false" ondrop="return false" onpaste="return false"></td>
                                                            </tr>
                                                            <tr>
                                                                <td style="width:35%;">RPGT Retention Sum (C)</td>
                                                                <td style="width:65%"><input class="form-control" id="retentionSum" name="retentionSum" type="text" placeholder="0.00" onchange="chgRetensionSum(this.value)" onkeypress="return onlyDotsAndNumbers(this,event);" oncopy="return false" ondrag="return false" ondrop="return false" onpaste="return false"></td>
                                                            </tr>
                                                            <tr>
                                                                <td style="width:35%;">Balance Deposit (D)</td>
                                                                <td style="width:65%"><input class="form-control" id="balanceDeposit" name="balanceDeposit" type="text" placeholder="0.00" onchange="chgBalanceDeposit(this.value)" onkeypress="return onlyDotsAndNumbers(this,event);" oncopy="return false" ondrag="return false" ondrop="return false" onpaste="return false"></td>
                                                            </tr>
                                                            <tr>
                                                                <td style="width:35%;">Total Deposit(E) = B+C+D</td>
                                                                <td style="width:65%"><input class="form-control" id="totalDeposit" name="totalDeposit" type="text" disabled placeholder="0.00"></td>
                                                            </tr>
                                                            <tr>
                                                                <td style="width:35%;">Balance Purchase Price = A-E=BPP</td>
                                                                <td style="width:65%"><input class="form-control" id="balancePurchasePrice" name="balancePurchasePrice" type="text" disabled placeholder="0.00" onkeypress="return onlyDotsAndNumbers(this,event);" oncopy="return false" ondrag="return false" ondrop="return false" onpaste="return false"></td>
                                                            </tr>
                                                            <tr>
                                                                <td style="width:35%;">Consumption Tax or GST (Tax)</td>
                                                                <td style="width:65%"><input class="form-control" id="consumptionTax" name="consumptionTax" type="text" placeholder="0.00" onkeypress="return onlyDotsAndNumbers(this,event);" oncopy="return false" ondrag="return false" ondrop="return false" onpaste="return false"></td>
                                                            </tr>
                                                            <tr>
                                                                <td style="width:35%;">Purchase Price After Tax</td>
                                                                <td style="width:65%"><input class="form-control" id="purchasePriceAfterTax" name="purchasePriceAfterTax" type="text" placeholder="0.00" onkeypress="return onlyDotsAndNumbers(this,event);" oncopy="return false" ondrag="return false" ondrop="return false" onpaste="return false"></td>
                                                            </tr>
                                                            <tr>
                                                                <td style="width:35%;">Adjustment Rate (Normally Project)</td>
                                                                <td style="width:65%"><input class="form-control" id="adjustmentRate" name="adjustmentRate" type="text" placeholder="0.00" onkeypress="return onlyDotsAndNumbers(this,event);" oncopy="return false" ondrag="return false" ondrop="return false" onpaste="return false"></td>
                                                            </tr>
                                                        </tbody>
