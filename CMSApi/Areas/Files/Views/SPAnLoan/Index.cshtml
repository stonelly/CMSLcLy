        function getFormattedDate(date) {
            var dateOpenFile = new Date(parseInt(date.replace("/Date(", "").replace(")/", ""), 10));
            var yyyy = dateOpenFile.getFullYear();
            let mm = dateOpenFile.getMonth() + 1; // Months start at 0!
            let dd = dateOpenFile.getDate();

            if (dd < 10) dd = '0' + dd;
            if (mm < 10) mm = '0' + mm;

            var formattedToday = dd + '/' + mm + '/' + yyyy;

            return formattedToday;
        }

