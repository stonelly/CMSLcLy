        function chgPurchasePrice(val) {
            var purchasePrice = document.getElementById("purchasePrice");
            var balanceDeposit = document.getElementById("balanceDeposit");
            var balancePurchasePrice = document.getElementById("balancePurchasePrice");

            purchasePrice.value = val;

            if (balanceDeposit.value == "") {
                balancePurchasePrice.value = parseInt(purchasePrice.value) - 0;
            } else {
                balancePurchasePrice.value = parseInt(purchasePrice.value) - parseInt(balanceDeposit.value);
            }
        }

        function chgEarnestDeposit(val) {
            var earnestDeposit = document.getElementById("earnestDeposit");
            var retentionSum = document.getElementById("retentionSum");
            var balanceDeposit = document.getElementById("balanceDeposit");
            var totalDeposit = document.getElementById("totalDeposit");
            var balancePurchasePrice = document.getElementById("balancePurchasePrice");

            earnestDeposit.value = val;

            var retention = 0
            var balDeposit = 0

            if (retentionSum.value != "") {
                retention = parseInt(retentionSum.value);
            }

            if (balanceDeposit.value != "") {
                balDeposit = parseInt(balanceDeposit.value);
            }

            totalDeposit.value = parseInt(earnestDeposit.value) + retention + balDeposit;

            if (balancePurchasePrice.value != "") {
                balancePurchasePrice.value = parseInt(balancePurchasePrice.value) - parseInt(totalDeposit.value);
            }
        }

        function chgRetensionSum(val) {
            var earnestDeposit = document.getElementById("earnestDeposit");
            var retentionSum = document.getElementById("retentionSum");
            var balanceDeposit = document.getElementById("balanceDeposit");
            var totalDeposit = document.getElementById("totalDeposit");
            var balancePurchasePrice = document.getElementById("balancePurchasePrice");

            retentionSum.value = val;

            var earnestDepo = 0
            var balDeposit = 0

            if (earnestDeposit.value != "") {
                earnestDepo = parseInt(earnestDeposit.value);
            }

            if (balanceDeposit.value != "") {
                balDeposit = parseInt(balanceDeposit.value);
            }

            totalDeposit.value = (parseInt(retentionSum.value) + earnestDepo + balDeposit);

            if (balancePurchasePrice.value != "") {
