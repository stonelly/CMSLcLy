        function chgBalanceDeposit(val) {
            var earnestDeposit = document.getElementById("earnestDeposit");
            var retentionSum = document.getElementById("retentionSum");
            var balanceDeposit = document.getElementById("balanceDeposit");
            var totalDeposit = document.getElementById("totalDeposit");
            var balancePurchasePrice = document.getElementById("balancePurchasePrice");

            balanceDeposit.value = val;

            var earnestDepo = 0
            var retention = 0

            if (earnestDeposit.value != "") {
                earnestDepo = parseInt(earnestDeposit.value);
            }

            if (retentionSum.value != "") {
                retention = parseInt(retentionSum.value);
            }

            totalDeposit.value = parseInt(balanceDeposit.value) + earnestDepo + retention;

            if (balancePurchasePrice.value != "") {
                balancePurchasePrice.value = parseInt(balancePurchasePrice.value) - parseInt(totalDeposit.value);
            }
        }

        function onlyDotsAndNumbers(txt, event) {
            var charCode = (event.which) ? event.which : event.keyCode
            if (charCode == 46) {
                if (txt.value.indexOf(".") < 0)
                    return true;
                else
                    return false;
            }

            if (txt.value.indexOf(".") > 0) {
                var txtlen = txt.value.length;
                var dotpos = txt.value.indexOf(".");
                //Change the number here to allow more decimal points than 2
                if ((txtlen - dotpos) > 2)
                    return false;
            }

            if (charCode > 31 && (charCode < 48 || charCode > 57))
                return false;

            return true;
        }

